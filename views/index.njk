{% extends "layout.njk" %}
{% block  content %}
<main>
    <h1>{{ title }}</h1>
    <p>{{ intro }}</p>
    <p>Detta är dock statisk text. Följt av data från databas.</p>
    
    {% for row in rows %}
        <ul>
            <div>
                <li>{{ row.name }}</li>
                <li>{{ row.secret_identity }}</li>
                <li>{{ row.bio }}</li>
            </div>
        </ul>
    {% endfor %}
    
   
</main>
{% endblock %}%